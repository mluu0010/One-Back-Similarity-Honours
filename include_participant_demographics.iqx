/ required = true
</dropdown>

<radiobuttons colourimpairment>
/caption = "Do you have any colour vision impairment?"
/required = true
/options = ("No","Yes")
/position = (values.demo_second_col, values.demo_starting_y_offset + 4*values.demo_gap)
</radiobuttons>

<textbox colour_response>
/caption = "If you have colour vision impairment, please describe the type (e.g. 'red-green colour blind'). Otherwise, skip this question."
/required = false
/ position = (values.demo_second_col, values.demo_starting_y_offset + 7.5*values.demo_gap)
</textbox>

<radiobuttons acuity>
