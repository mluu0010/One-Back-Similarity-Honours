<values>
/demo_first_col = 10
/demo_second_col = 55
/demo_starting_y_offset = 20
/demo_gap = 5
</values>

<radiobuttons sex>
/ caption = "Sex"
/ options = ("Male", "Female", "Other", "Prefer Not to Answer")
/ orientation = horizontal
/ position = (values.demo_first_col, values.demo_starting_y_offset)
/ responsefontstyle = ("Arial", 2.5%, false, false, false, false, 5, 0)
/ required = true
</radiobuttons>

<textbox age>
/ caption = "Age (years)"
/ maxchars = 3
/ textboxsize = (6, 4)
/ required = true
/ mask = positiveintegerorzero
/ position = (values.demo_first_col, values.demo_starting_y_offset + 2.5*values.demo_gap)
